head
  meta(charset="utf-8")
  script(src="http://code.angularjs.org/1.4.7/angular.min.js")
  script(type="application/javascript", src="lib/jquery.min.js")
    
body(ng-app="scheduleApp" ng-controller="scheduleCtrl")
  h1 行程修改
  div
    select(ng-model="artist_ID" ng-options="artist.name for artist in artists")
  div
    table
      thead
        tr
          th(data-dynatable-column="time") 时间
          th(data-dynatable-column="schedules_text") 内容
  div
    p 时间
      textarea 
    p 内容 
      textarea
    input(type="button", 
          name="Insert",
          value="上传",
          )
         
            
      
script(type="application/javascript", src="js/schedule.js")
